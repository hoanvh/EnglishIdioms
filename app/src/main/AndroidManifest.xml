<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="binh.app.englishidiom"
    android:versionCode="8"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="binh.app.englishidiom.Main"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="binh.app.englishidiom.CommonActivity"></activity>
        <activity android:name="binh.app.englishidiom.ContentSlangActivity"></activity>
        <activity android:name="binh.app.englishidiom.SlangActivity"></activity>
        <activity android:name="binh.app.englishidiom.ActivityMain" />
        <activity android:name="binh.app.englishidiom.ContentBookMarkActivity" />
        <activity android:name="binh.app.englishidiom.ParseActivity" />
        <activity android:name="binh.app.englishidiom.ContentActivity" />
        <activity android:name="binh.app.englishidiom.BookMarkActivity" />
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
